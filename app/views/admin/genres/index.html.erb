<h3>ジャンル一覧・追加</h3>
<%= form_with model: @genre,url: admin_genres_path, method: :post do |f| %>
  <%= f.label :"ジャンル名" %><br>
  <%= f.text_field :name %><br>
  <%= f.submit'新規登録' %><br>
<% end %>
ジャンル<br>
<% @genres.each do |genre| %>
  <%= genre.name %>
  <%= link_to "編集する", edit_admin_genre_path(genre.id) %><br>
<% end %>