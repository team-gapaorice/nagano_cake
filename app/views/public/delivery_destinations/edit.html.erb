<div class="mx-5 py-5">
  <h3>配送先編集</h3>
  <%= form_with model: @delivery_destination,url: delivery_destination_path, method: :patch do |f| %>
    <%= f.label :"郵便番号（ハイフンなし）" %><br>
    <%= f.text_field :postal_code %><br>
    <%= f.label :"住所" %><br>
    <%= f.text_field :adress %><br>
    <%= f.label :"宛名" %><br>
    <%= f.text_field :name %>
    <%= f.submit'変更内容を保存' %><br>
  <% end %>
</div>